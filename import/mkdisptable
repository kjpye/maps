drop table if exists displayorder;
create table displayorder (
  displayorder  int default 0,
  tablename     text,
  featurename   text,
  drawtype      text,
  featurecolumn text default '',
  geomcol       text default 'geom',
  orientcol     text default '',
  widthcol      text default '',
  defaultsymbol int default 0
);
grant select on displayorder to ro;

